{
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "~constants": "./client/constants",
          "~components": "./client/components",
          "~helpers": "./client/helpers"
        }
      }
    ]
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-env" 
  ]
}
